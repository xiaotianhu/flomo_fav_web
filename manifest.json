{
  "manifest_version": 2,
  "name"            : "PageToFlomo",
  "description"     : "clip web for flomo.app",
  "version"         : "1.0",
  "browser_action"  : {
      "default_icon" : "img/logo.png",
      "default_title": "Clip web for flomo.app",
      "default_popup": "popup.html"
  },
  "permissions": ["tabs", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*", "activeTab", "storage"],
  "web_accessible_resources": ["index.js", "popup.html", "js/common.js", "img/msg.png", "js/jquery.js"]
}
